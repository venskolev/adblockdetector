{
    "name": "vkolev/adblockdetector",
    "type": "phpbb-extension",
    "description": "Detects ad blockers and applies custom logic in phpBB.",
    "homepage": "https://github.com/venskolev/adblockdetector",
    "version": "1.0.0",
    "time": "2024-03-05",
    "keywords": [
        "phpbb",
        "extension",
        "adblock",
        "detector"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Ventsislav Kolev",
            "email": "v.kolev@alfatrex.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phpbb/phpbb": ">=3.3",
        "composer/installers": "~1.0.0"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "AdBlock Detector",
        "soft-require": {
            "phpbb/phpbb": ">=3.3"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/adblock_detector",
            "filename": "version_check",
            "ssl": true
        }
    }
}
